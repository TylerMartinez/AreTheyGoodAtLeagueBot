{
    "family": "DeployBot",
    "containerDefinitions": [
        {
            "name": "AreTheyGoodAtLeagueBot",
            "image": "",
            "cpu": 512,
            "memory": 1024,
            "essential": true,
            "secrets": [
                {
                    "name": "",
                    "valueFrom": ""
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/tyler",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "environment": [
                {
                    "name": "DISCORD_TOKEN",
                    "value": "<discord_secret>"
                },
                {
                    "name": "RIOT_TOKEN",
                    "value": "<riot_secret>"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}